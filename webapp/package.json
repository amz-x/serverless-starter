{
  "name": "serverless-webapp",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve --host 127.0.0.1 --port 4000",
    "watch": "npm run config -- --environment=dev && ng build --watch --configuration development",
    "build": "npm run config -- --environment=prod && ng build",
    "test": "ng test",
    "test:run": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "ng lint"
  },
